{
  "name": "tecnasa-restapi-for-android-app",
  "version": "0.1.0",
  "bin": {
    "tecnasa-restapi": "bin/tecnasa-restapi.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
